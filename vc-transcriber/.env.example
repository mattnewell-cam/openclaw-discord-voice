DISCORD_TOKEN=
GUILD_ID=
VOICE_CHANNEL_ID=
TEXT_CHANNEL_ID=

# Python/Whisper
WHISPER_PYTHON=./.venv/bin/python
WHISPER_MODEL=small
WHISPER_LANGUAGE=en
WHISPER_DEVICE=cpu
WHISPER_COMPUTE_TYPE=int8

# Audio segmentation
SILENCE_DURATION_MS=800
MIN_SPEECH_MS=600

# Optional: only transcribe these users (comma-separated)
SPEAK_USER_IDS=

# Optional: ignore specific users (comma-separated)
IGNORE_USER_IDS=

# Start/stop gating (anyone can say start/stop)
TRANSCRIBE_REQUIRE_START=false
TRANSCRIBE_EXACT_MATCH=false
TRANSCRIBE_START_PHRASES=start message
TRANSCRIBE_STOP_PHRASES=stop message,end message

# Filter filler-only transcripts (mm/ah/oh/yeah)
FILTER_FILLER=true
DEBUG_VC=true
