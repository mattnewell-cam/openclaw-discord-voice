# Core
DISCORD_TOKEN=
GUILD_ID=
VOICE_CHANNEL_ID=
TEXT_CHANNEL_ID=
YOUR_USER_ID=
OPENCLAW_BOT_ID=

# Python
PYTHON=./.venv/bin/python

# Whisper
WHISPER_MODEL=tiny
WHISPER_LANGUAGE=en
WHISPER_DEVICE=cpu
WHISPER_COMPUTE_TYPE=int8

# Audio segmentation
SILENCE_DURATION_MS=800
MIN_SPEECH_MS=600

# Start/stop beeps
BEEP_DURATION=0.35
BEEP_AMPLITUDE=0.9

# Allow bot messages to be spoken in SPEAK_CHANNEL_ID
ALLOW_BOT_MESSAGES=true

# TTS provider: edge (default) or piper
TTS_PROVIDER=edge

# Start/stop gating (anyone can say start/stop)
TRANSCRIBE_START_PHRASES=start message,stop message,so message,start the message,star message,strong message
TRANSCRIBE_STOP_PHRASES=end message,and message

# Filter filler-only transcripts (mm/ah/oh/yeah)
FILTER_FILLER=true
DEBUG_VC=true
TIMING_LOG=false
