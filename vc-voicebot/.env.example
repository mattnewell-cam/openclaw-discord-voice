# Core
DISCORD_TOKEN=
GUILD_ID=
VOICE_CHANNEL_ID=
TEXT_CHANNEL_ID=
YOUR_USER_ID=
OPENCLAW_BOT_ID=

# Python/Whisper
WHISPER_PYTHON=./.venv/bin/python
WHISPER_MODEL=tiny
WHISPER_LANGUAGE=en
WHISPER_DEVICE=cpu
WHISPER_COMPUTE_TYPE=int8

# Audio segmentation
SILENCE_DURATION_MS=800
MIN_SPEECH_MS=600

# Start/stop beeps
BEEP_DURATION=0.35
BEEP_AMPLITUDE=0.9

# Allow bot messages to be spoken in SPEAK_CHANNEL_ID
ALLOW_BOT_MESSAGES=true

# TTS provider: edge (default) or piper
TTS_PROVIDER=edge
TTS_PYTHON=./.venv/bin/python

# Edge TTS settings
EDGE_VOICE=en-GB-RyanNeural
EDGE_RATE=+0%
EDGE_PITCH=+0Hz
EDGE_VOLUME=+0%

# Piper (optional fallback)
PIPER_MODEL=voices/en_GB-northern_english_male-medium.onnx
PIPER_DATA_DIR=voices
PIPER_SPEAKER=
PIPER_LENGTH_SCALE=0.95
PIPER_NOISE_SCALE=
PIPER_NOISE_W_SCALE=
PIPER_SENTENCE_SILENCE=0.12

# Optional: max characters per chunk
MAX_CHUNK=400

# Start/stop gating (anyone can say start/stop)
TRANSCRIBE_EXACT_MATCH=false
TRANSCRIBE_START_PHRASES=start message,stop message
TRANSCRIBE_STOP_PHRASES=end message,and message

# Auto-leave when specific user leaves VC
LEAVE_ON_USER_IDS=

# Filter filler-only transcripts (mm/ah/oh/yeah)
FILTER_FILLER=true
DEBUG_VC=true
TIMING_LOG=false
